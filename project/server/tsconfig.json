{
  "compilerOptions": {         
    "module": "commonjs",
		"noImplicitReturns": true,
		"noUnusedLocals": false,
		"outDir": "dist",
		"sourceMap": true,
		"strict": true,
		"target": "es5",
		"lib": [
			"es2019",
			"DOM"
		],
		"noImplicitAny": false,
		"downlevelIteration": true,
		"alwaysStrict": true
	},
	"compileOnSave": true,                  /* Skip type checking all .d.ts files. */
  }

